<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-Information" path="/I2CE">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="person-Information" path="/I2CE" locale="en_US">
    <version>4.2.0</version>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="person_Information">
          <configurationGroup name="fields">
            <configurationGroup name="residence">
              <configuration name="headers" type="delimited" values="many" locale="en_US">
                <value>default:District of Origin</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="academic_edu_lev">
              <configuration name="headers" type="delimited" locale="en_US" values="single">
                <value>default:Highest Education Level</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="prof_qualification">
              <configuration name="headers" type="delimited" locale="en_US" values="single">
                <value>default:Major in Education</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="nametitle">
              <configuration name="headers" type="delimited" locale="en_US" values="single">
                <value>default:Prefix</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="datefirstappointment">
              <configuration name="headers" type="delimited" locale="en_US" values="single">
                <value>default:Date of First Appointment</value>
              </configuration>
            </configurationGroup>
            <!--
             <configurationGroup name="maidenname">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Maiden Name</value>
              </configuration>
            </configurationGroup>
            -->
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>